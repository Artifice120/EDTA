process {
    withName: 'LTRHARVEST' {
        ext.args = '-size 1000000 -time 300'
    }
}

process {
    withName: 'LTRFINDER' {
        ext.args = '-harvest_out -size 1000000 -time 300'
    }
}

process {
    withName: 'ANNOSINE' {
        ext.args = '--num_alignments 50000 -rpm 0 --copy_number 3 --shift 100 -auto 1'
    }
}